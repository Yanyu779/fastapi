# Apollo配置中心连接配置
# 复制此文件为 .env 并修改相应的配置

# Apollo服务器地址
APOLLO_SERVER_URL=http://localhost:8080

# 应用ID
APOLLO_APP_ID=demo-app

# 集群名称
APOLLO_CLUSTER=default

# 命名空间
APOLLO_NAMESPACE=application

# 是否启用长轮询
APOLLO_ENABLE_LONG_POLLING=true

# 轮询间隔（秒）
APOLLO_POLLING_INTERVAL=30

# 超时时间（秒）
APOLLO_TIMEOUT=30

# 是否启用缓存
APOLLO_ENABLE_CACHE=true

# 缓存目录
APOLLO_CACHE_DIR=./apollo_cache